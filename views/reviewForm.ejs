        <div id="review" class="card">
            <h1 class="card-title">Review</h1>
            <div class="card-body">
                <form class="rating" method="POST" action="/review">
                        <label for="charitylist" class="form-label"></label>
                        <select id="charitylist" name="selectedCharity" class="form-select" required>
                            <option selected disabled>Choose your charity</option>
                            <option value="etaam">Etaam</option>
                            <option value="albir">Albir</option>
                            <option value="nemah">Nemah</option>
                            <option value="ekram">Ekram</option>
                        </select>
                    <div class="mb-3 mt-3">
                        <textarea rows="4" class="form-control" id="message" placeholder="Your review" name="message" required></textarea>
                    </div>
                    <button id="submitbtn" type="submit" class="btn">Submit</button>
                </form>
            </div>

        </div>
